<h1>new ad</h1>

<%= form_for @ad, :url=>{:action=>'create'} do |f| %>

<p><b>Name</b><br /><%= f.text_field :name %></p>

<p><b>Description</b><br /><%= f.text_area :description%></p>

<p><b>Price</b><%=f.text_field :price%></p>

<p><b>Seller</b><%=f.text_field :seller_id%></p>

<p><b>Email</b><%=f.text_field :email%></p>

<p><b>Img url</b><%=f.text_field :img_url%></p>

<p><%=f.submit "Create"%></p>

<% end %>


